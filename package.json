{
  "name": "minapp-ui-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "build": "vue-cli-service build --modern",
    "build:staging": "vue-cli-service build --mode staging --modern",
    "deploy:ghpages": "",
    "deploy:oss": "vue-cli-service build --modern && vue-cli-service deploy-oss",
    "deploy:surge": "vue-cli-service build --mode staging --modern && vue-cli-service deploy-surge"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.4",
    "dingtalk-jsapi": "^2.11.2",
    "lodash": "^4.17.15",
    "register-service-worker": "^1.7.1",
    "rxjs": "^6.5.5",
    "vant": "^2.8.6",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vue-wechat-title": "^2.0.5",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-pwa": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-unit-mocha": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "ali-oss": "^6.9.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "chai": "^4.1.2",
    "cross-env": "^7.0.2",
    "date-format": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "github-pages-publisher": "^0.0.4",
    "globby": "^11.0.1",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "surge": "^0.21.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "iOS >= 7",
    "Android >= 4.0",
    "last 1 versions"
  ],
  "vuePlugins": {
    "service": [
      "bin/deploy-oss.js",
      "bin/deploy-surge.js"
    ]
  }
}
